#: argparse.py:294
msgid "usage: "
msgstr "uzado: "

#: argparse.py:836
msgid ".__call__() not defined"
msgstr ".__call__() ne difiniĝas"

#: argparse.py:1139
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "nekonata analizilo %(parser_name)r (elektoj: %(choices)s)"

#: argparse.py:1193
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "argumento \"-\" kun moduso %r"

#: argparse.py:1201
#, python-format
msgid "can't open '%s': %s"
msgstr "ne povas malfermi '%s': %s"

#: argparse.py:1405
#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr "ne povas kunigi agojn - du grupoj nomiĝas %r"

#: argparse.py:1443
msgid "'required' is an invalid argument for positionals"
msgstr "'required' estas nevalida argumento por poziciaj"

#: argparse.py:1465
#, python-format
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"nevalida elektebla ĉeno %(option)r: devas komenciĝi per signo "
"%(prefix_chars)r"

#: argparse.py:1485
#, python-format
msgid "dest= is required for options like %r"
msgstr "dest= nepras por elektebloj kiel %r"

#: argparse.py:1502
#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "nevalida valoro de conflict_resolution: %r"

#: argparse.py:1520
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "konfliktanta elektebla ĉeno: %s"
msgstr[1] "konfliktantaj elekteblaj ĉenoj: %s"

#: argparse.py:1586
msgid "mutually exclusive arguments must be optional"
msgstr "reciproke ekskluzivaj argumentoj devas esti malnepraj"

#: argparse.py:1649
msgid "positional arguments"
msgstr "poziciaj argumentoj"

#: argparse.py:1650
msgid "optional arguments"
msgstr "malnepraj argumentoj"

#: argparse.py:1665
msgid "show this help message and exit"
msgstr "montri ĉi tiun helpmesaĝon kaj eliri"

#: argparse.py:1696
msgid "cannot have multiple subparser arguments"
msgstr "ne povas havi plurajn subanalizilajn argumentojn"

#: argparse.py:1748 argparse.py:2254
#, python-format
msgid "unrecognized arguments: %s"
msgstr "nekonataj argumentoj: %s"

#: argparse.py:1845
#, python-format
msgid "not allowed with argument %s"
msgstr "ne permesita kun argumento: %s"

#: argparse.py:1891 argparse.py:1905
#, python-format
msgid "ignored explicit argument %r"
msgstr "ignoris malimplicitan argumenton %r"

#: argparse.py:2012
#, python-format
msgid "the following arguments are required: %s"
msgstr "la sekvaj argumentoj nepras: %s"

#: argparse.py:2027
#, python-format
msgid "one of the arguments %s is required"
msgstr "unu el la argumentoj %s nepras"

#: argparse.py:2070
msgid "expected one argument"
msgstr "atendis unu argumenton"

#: argparse.py:2071
msgid "expected at most one argument"
msgstr "atendis maksimume unu argumenton"

#: argparse.py:2072
msgid "expected at least one argument"
msgstr "atendis minimume unu argumenton"

#: argparse.py:2074
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "atendis %s argumenton"
msgstr[1] "atendis %s argumentojn"

#: argparse.py:2134
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "dubsenca elekteblo: %(option)s povus egali al %(matches)s"

#: argparse.py:2197
#, python-format
msgid "unexpected option string: %s"
msgstr "neatendita elektebla ĉeno: %s"

#: argparse.py:2394
#, python-format
msgid "%r is not callable"
msgstr "%r ne alvokeblas"

#: argparse.py:2411
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "nevalida %(type)s-valoro: %(value)r"

#: argparse.py:2422
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "nevalida elekto: %(value)r (elektu el %(choices)s)"

#: argparse.py:2498
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: eraro: %(message)s\n"
